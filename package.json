{
  "name": "@resusio/simlab",
  "version": "0.2.2",
  "description": "A library to generate mock patient laboratory test reports, for use in medical simulation teaching.",
  "homepage": "https://github.com/resusio/simlab#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "typedoc --out docs --theme minimal src",
    "docs:clean": "rm -rf ./docs",
    "fmt:check": "prettier --check \"src/**/*\"",
    "fmt:write": "prettier --write \"src/**/*\"",
    "clean": "rm -rf ./dist",
    "build": "tsc",
    "prod": "node ./dist/index.js",
    "dev": "nodemon",
    "start": "npm run dev"
  },
  "author": "Tristan Jones <jones.tristand@gmail.com> (https://www.tdjones.ca/)",
  "license": "MIT",
  "dependencies": {
    "random": "^2.1.1",
    "underscore": "^1.9.2",
    "underscore.get": "^0.2.9"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@types/random": "^2.1.1",
    "@types/underscore": "^1.10.24",
    "nodemon": "^2.0.6",
    "prettier": "^1.19.1",
    "ts-node": "^9.1.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resusio/simlab.git"
  },
  "bugs": {
    "url": "https://github.com/resusio/simlab.git"
  }
}
